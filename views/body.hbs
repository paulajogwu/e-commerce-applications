              <a class="btn"  href="#"  data-id="{{id}}" id="item">Add to
                <i class="icon-shopping-cart"></i></a>
              <a class="btn btn-primary" href="#">{{currency}}
                {{price}}</a></h4>
          </div>
        </div>
      </li>
    {{/each}}

  </ul>

</div>

